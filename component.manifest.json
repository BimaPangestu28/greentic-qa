    "host": {
      "messaging": {
        "inbound": true,
        "outbound": true
      },
      "qa": {
        "process": true
      },
      "secrets": {
        "required": []
      },
      "telemetry": {
        "scope": "node"
      }
    },
    "wasi": {
      "clocks": true,
      "filesystem": {
        "mode": "none",
        "mounts": []
      },
      "random": true
    }
  },
  "config_schema": {
    "$ref": "schemas/io/input.schema.json"
  },
  "operations": [
    {
      "name": "describe",
      "input_schema": {
